# Install command-line tools using Homebrew
# Usage: `brew bundle Brewfile`

tap caskroom/cask
install brew-cask

# Update and upgrade. 
update
upgrade

install git
install git-flow

cask install google-chrome
cask install atom
cask install dropbox
cask install firefox
cask install virtualbox
cask install vagrant
cask install vlc
cask install skype
